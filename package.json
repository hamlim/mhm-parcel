{
  "name": "mhme-parcel",
  "private": true,
  "version": "0.0.0",
  "now": {
    "alias": "matthamlin.me"
  },
  "scripts": {
    "dev": "parcel ./src/index.html",
    "make": "parcel build ./src/index.html --out-dir build",
    "postmake": "cp -a ./build/. ./site/",
    "deploy": "now",
    "start": "serve ./site/ -s"
  },
  "devDependencies": {
    "@babel/cli": "7.1.5",
    "@babel/core": "7.1.5",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/preset-env": "7.1.5",
    "@babel/preset-react": "7.0.0",
    "@matthamlin/parcel-plugin-mdx": "1.2.0",
    "parcel-bundler": "1.10.3",
    "serve": "10.0.2"
  },
  "dependencies": {
    "@compositor/markdown": "^0.0.21",
    "@emotion/core": "10.0.0-beta.13",
    "@emotion/styled": "10.0.0-beta.13",
    "@matthamlin/react-error-boundary": "^1.0.0",
    "@matthamlin/resp": "^3.0.0",
    "@mdx-js/tag": "0.15.6",
    "@reach/router": "1.2.1",
    "date-fns": "^1.29.0",
    "emotion": "^9.2.12",
    "grid-emotion": "^3.2.0",
    "highlight.js": "9.13.1",
    "marked": "^0.3.19",
    "normalize.css": "8.0.0",
    "react": "16.7.0-alpha.0",
    "react-dom": "16.7.0-alpha.0",
    "react-router-dom": "^4.4.0-beta.6",
    "react-state-reducer": "1.4.0",
    "react-transition-group": "2.5.0",
    "reason-react": "^0.3.1",
    "recycles": "^1.1.0",
    "remark-highlight.js": "5.0.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  }
}
