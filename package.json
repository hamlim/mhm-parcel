{
  "name": "mhme-parcel",
  "version": "0.0.0",
  "scripts": {
    "dev": "parcel ./src/index.html",
    "dev-reason": "yarn watch & parcel ./src/index.html",
    "watch": "bsb -make-world -w",
    "make": "parcel build ./src/index.html --out-dir build && cp ./build/index.html ./index.html",
    "deploy": "now",
    "start": "serve"
  },
  "devDependencies": {
    "bs-platform": "^2.1.0",
    "bsb-js": "^1.0.2",
    "parcel-bundler": "^1.5.1",
    "parcel-plugin-markdown": "0.2.1"
  },
  "dependencies": {
    "@atlaskit/avatar": "9.0.0",
    "@compositor/markdown": "^0.0.21",
    "@matthamlin/react-error-boundary": "^1.0.0",
    "@matthamlin/resp": "^2.0.0",
    "babel-plugin-macros": "2.1.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "date-fns": "^1.29.0",
    "emotion": "^8.0.12",
    "markdown-to-jsx": "6.4.1",
    "marked": "0.3.12",
    "normalize.css": "7.0.0",
    "preval.macro": "1.0.2",
    "prop-types": "15.6.0",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-emotion": "^8.0.12",
    "react-router-dom": "^4.2.2",
    "react-transition-group": "2.2.1",
    "reason-react": "^0.3.1",
    "remark": "^9.0.0",
    "remark-react": "^4.0.1",
    "serve": "6.5.3"
  }
}
